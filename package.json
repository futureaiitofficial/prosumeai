    "sync-invoices": "node server/utils/run-ts-script.js server/scripts/sync-subscription-invoices.ts",
    "update-tx-labels": "node server/utils/run-ts-script.js server/scripts/update-transaction-labels.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:cookies": "jest cookie-manager",
    "test:cookie-stress": "tsx server/utils/cookie-stress-test.ts",
    "db:seed": "cd server && npm run db:seed",
    "generate-types": "npx drizzle-kit generate:pg --schema=./shared/schema.ts",
    "check-types": "tsc --noEmit",
    "migrate": "node server/drizzle/migrate.js",
    "push": "node server/drizzle/push.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "migrate:branding": "node server/migrations/add-branding-settings.cjs",
    "migrate:smtp": "node server/migrations/add-smtp-settings.cjs",
    "migrate:tax": "node server/migrations/add-tax-system.cjs",
