    "build:client": "npx vite build --config vite.config.ts",
    "build:server": "npx tsc --project tsconfig.server.json",
    "build:ensure-dist": "node server/utils/ensure-dist-dir.js",
    "start": "node start-dev.js",
    "start:prod": "node start-prod.js",
    "start:dev": "tsx server/index.ts",
    "check:client": "npx tsc --project tsconfig.client.json --noEmit",
    "check:server": "npx tsc --project tsconfig.server.json --noEmit",
    "check": "npm run check:client && npm run check:server",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx server/migrations/run_migration.ts",
    "db:migrate:security": "tsx server/migrations/run-security-migrations.ts",
    "db:migrate:invoice-fields": "tsx server/migrations/run-invoice-fields-migration.ts",
