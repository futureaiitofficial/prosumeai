    },
    "testMatch": [
      "**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "server/utils/**/*.ts",
      "!server/utils/**/*.test.ts",
      "!server/utils/**/cookie-stress-test.ts"
    ],
