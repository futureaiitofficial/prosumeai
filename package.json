  "scripts": {
    "dev": "node start-dev.js",
    "dev:tunnel": "node tunnel-dev.js",
    "dev:serveo": "node serveo-dev.js",
    "dev:client": "vite --config vite.config.ts",
    "dev:server": "tsx watch server/index.ts",
